<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
  <title><%= title %></title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="container">
  <%= render 'layouts/header' %>

  <p class="notice"><%= notice %></P>
  <p class="alert"><%= alert %></P>

  <%= render 'layouts/navigation' %>

  <%= yield %>

  <%= render 'layouts/footer' %>
</div>

</body>
</html>
